# What:       libncurses
# Is a:       Port
# Maintainer: Matthew Iselin

pkgname=libncurses
pkgver=5.7
pkgrel=1
pkgdesc="The ncurses libraries"
arch=('i686')
url=""
license=('GPL')
groups=()
depends=()
makedepends=()
provides=()
conflicts=()
replaces=()
backup=()
install=
source=($pkgname-$pkgver.tar.gz)
noextract=()
md5sums=() #generate with 'makepkg -g'

build() {
  cd "$srcdir/$pkgname-$pkgver"
  cp -R ./* $pkgdir/
}