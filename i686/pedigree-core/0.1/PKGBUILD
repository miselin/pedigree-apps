# Pedigree PKGBUILD Script
#
# What:       Pedigree Core
# Is a:       Native Application
# Maintainer: Matthew Iselin

pkgname=pedigree-core
pkgver=0.1
pkgrel=1
pkgdesc="Pedigree Core"
arch=('i686')
url="http://pedigree-project.org"
license=('New BSD')
groups=()
depends=('')
makedepends=()
provides=()
conflicts=()
replaces=()
backup=()
install=
source=($pkgname-$pkgver.tar.gz)
noextract=()
md5sums=() #generate with 'makepkg -g'

build() {
  cd "$srcdir/$pkgname-$pkgver"
  cp -R ./* $pkgdir/
}