# Pedigree PKGBUILD Script
#
# What:       OpenSSL
# Is a:       Port
# Maintainer: Matthew Iselin

pkgname=openssl
pkgver=0.9.8k
pkgrel=1
pkgdesc="OpenSSL: SSL utilities"
arch=('i686')
url=""
license=('GPL')
groups=()
depends=()
makedepends=()
provides=()
conflicts=()
replaces=()
backup=()
install=
source=($pkgname-$pkgver.tar.gz)
noextract=()
md5sums=() #generate with 'makepkg -g'

build() {
  cd "$srcdir/$pkgname-$pkgver"
  cp -R ./* $pkgdir/
}