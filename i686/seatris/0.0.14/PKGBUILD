# Pedigree PKGBUILD Script
#
# What:       seatris
# Is a:       Port
# Maintainer: Matthew Iselin

pkgname=seatris
pkgver=0.0.14
pkgrel=1
pkgdesc="Seatris: ncurses-based tetris clone"
arch=('i686')
url=""
license=('GPL')
groups=()
depends=()
makedepends=()
provides=()
conflicts=()
replaces=()
backup=()
install=
source=($pkgname-$pkgver.tar.gz)
noextract=()
md5sums=() #generate with 'makepkg -g'

build() {
  cd "$srcdir/$pkgname-$pkgver"
  cp -R ./* $pkgdir/
}