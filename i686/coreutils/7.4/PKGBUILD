# Pedigree PKGBUILD Script
#
# What:       Coreutils
# Is a:       Port
# Maintainer: Matthew Iselin

pkgname=coreutils
pkgver=7.4
pkgrel=1
pkgdesc="Coreutils"
arch=('i686')
url="http://www.gnu.org/software/coreutils/"
license=('GPL')
groups=()
depends=()
makedepends=()
provides=()
conflicts=()
replaces=()
backup=()
install=
source=($pkgname-$pkgver.tar.gz)
noextract=()
md5sums=() #generate with 'makepkg -g'

build() {
  cd "$srcdir/$pkgname-$pkgver"
  cp -R ./* $pkgdir/
}