# Pedigree PKGBUILD Script
#
# What:       GCC
# Is a:       Port
# Maintainer: Matthew Iselin

pkgname=gcc
pkgver=4.3.2
pkgrel=1
pkgdesc="GNU Compiler Collection"
arch=('i686')
url="http://gcc.gnu.org/"
license=('GPL')
groups=()
depends=()
makedepends=()
provides=()
conflicts=()
replaces=()
backup=()
install=
source=($pkgname-$pkgver.tar.gz)
noextract=()
md5sums=() #generate with 'makepkg -g'

build() {
  cd "$srcdir/$pkgname-$pkgver"
  cp -R ./* $pkgdir/
}