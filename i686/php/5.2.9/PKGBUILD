# Pedigree PKGBUILD Script
#
# What:       PHP
# Is a:       Port
# Maintainer: Matthew Iselin

pkgname=php
pkgver=5.2.9
pkgrel=1
pkgdesc="PHP Hypertext Processor"
arch=('i686')
url=""
license=('GPL')
groups=()
depends=()
makedepends=()
provides=()
conflicts=()
replaces=()
backup=()
install=
source=($pkgname-$pkgver.tar.gz)
noextract=()
md5sums=() #generate with 'makepkg -g'

build() {
  cd "$srcdir/$pkgname-$pkgver"
  cp -R ./* $pkgdir/
}