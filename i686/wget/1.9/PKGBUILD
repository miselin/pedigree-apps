# Pedigree PKGBUILD Script
#
# What:       wget
# Is a:       Port
# Maintainer: Matthew Iselin

pkgname=wget
pkgver=1.9
pkgrel=1
pkgdesc="wget"
arch=('i686')
url="http://www.gnu.org/software/wget/"
license=('GPL')
groups=()
depends=()
makedepends=()
provides=()
conflicts=()
replaces=()
backup=()
install=
source=($pkgname-$pkgver.tar.gz)
noextract=()
md5sums=() #generate with 'makepkg -g'

build() {
  cd "$srcdir/$pkgname-$pkgver"
  cp -R ./* $pkgdir/
}