# Pedigree PKGBUILD Script
#
# What:       vim
# Is a:       Port
# Maintainer: James Molloy

pkgname=vim
pkgver=7.2
pkgrel=1
pkgdesc="vim"
arch=('i686')
url="http://www.vim.org/"
license=('GPL')
groups=()
depends=()
makedepends=()
provides=()
conflicts=()
replaces=()
backup=()
install=
source=($pkgname-$pkgver.tar.gz)
noextract=()
md5sums=() #generate with 'makepkg -g'

build() {
  cd "$srcdir/$pkgname-$pkgver"
  cp -R ./* $pkgdir/
}