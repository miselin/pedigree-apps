# Pedigree PKGBUILD Script
#
# What:       Ruby 1.8
# Is a:       Port
# Maintainer: James Molloy

pkgname=ruby
pkgver=1.8
pkgrel=1
pkgdesc="Ruby"
arch=('i686')
url=""
license=('GPL')
groups=()
depends=()
makedepends=()
provides=()
conflicts=()
replaces=()
backup=()
install=
source=($pkgname-$pkgver.tar.gz)
noextract=()
md5sums=() #generate with 'makepkg -g'

build() {
  cd "$srcdir/$pkgname-$pkgver"
  cp -R ./* $pkgdir/
}