# Pedigree PKGBUILD Script
#
# What:       ext2 utilities
# Is a:       Port
# Maintainer: James Molloy

pkgname=ext2
pkgver=unknown
pkgrel=1
pkgdesc="EXT2 Filesystem utilities"
arch=('i686')
url=""
license=('GPL')
groups=()
depends=()
makedepends=()
provides=()
conflicts=()
replaces=()
backup=()
install=
source=($pkgname-$pkgver.tar.gz)
noextract=()
md5sums=() #generate with 'makepkg -g'

build() {
  cd "$srcdir/$pkgname-$pkgver"
  cp -R ./* $pkgdir/
}