# Pedigree PKGBUILD Script
#
# What:       make
# Is a:       Port
# Maintainer: Matthew Iselin

pkgname=make
pkgver=3.81
pkgrel=1
pkgdesc="GNU make"
arch=('i686')
url=""
license=('GPL')
groups=()
depends=()
makedepends=()
provides=()
conflicts=()
replaces=()
backup=()
install=
source=($pkgname-$pkgver.tar.gz)
noextract=()
md5sums=() #generate with 'makepkg -g'

build() {
  cd "$srcdir/$pkgname-$pkgver"
  cp -R ./* $pkgdir/
}