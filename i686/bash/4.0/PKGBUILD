# Pedigree PKGBUILD Script
#
# What:       bash
# Is a:       Port
# Maintainer: Matthew Iselin

pkgname=bash
pkgver=4.0
pkgrel=1
pkgdesc="The Bourne Again Shell"
arch=('i686')
url=""
license=('GPL')
groups=()
depends=()
makedepends=()
provides=()
conflicts=()
replaces=()
backup=()
install=
source=($pkgname-$pkgver.tar.gz)
noextract=()
md5sums=() #generate with 'makepkg -g'

build() {
  cd "$srcdir/$pkgname-$pkgver"
  cp -R ./* $pkgdir/
}