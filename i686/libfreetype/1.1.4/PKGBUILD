# Pedigree PKGBUILD Script
#
# What:       libfreetype
# Is a:       Port
# Maintainer: James Molloy

pkgname=libfreetype
pkgver=1.1.4
pkgrel=1
pkgdesc="libfreetype"
arch=('i686')
url=""
license=('BSD')
groups=()
depends=()
makedepends=()
provides=()
conflicts=()
replaces=()
backup=()
install=
source=($pkgname-$pkgver.tar.gz)
noextract=()
md5sums=() #generate with 'makepkg -g'

build() {
  cd "$srcdir/$pkgname-$pkgver"
  cp -R ./* $pkgdir/
}