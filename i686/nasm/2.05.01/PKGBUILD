pkgname=nasm
pkgver=2.05.01
pkgrel=1
pkgdesc="The Netwide Assembler"
arch=('i686')
url="http://www.nasm.us/"
license=('GPL')
groups=()
depends=()
makedepends=()
provides=()
conflicts=()
replaces=()
backup=()
install=
source=($pkgname-$pkgver.tar.gz)
noextract=()
md5sums=() #generate with 'makepkg -g'

build() {
  cd "$srcdir/$pkgname-$pkgver"
  cp -R ./* $pkgdir/
}