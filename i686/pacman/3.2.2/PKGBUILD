# Pedigree PKGBUILD Script
#
# What:       pacman
# Is a:       Port
# Maintainer: Matthew Iselin

pkgname=pacman
pkgver=3.2.2
pkgrel=1
pkgdesc="Arch Linux package manager"
arch=('i686')
url=""
license=('GPL')
groups=()
depends=()
makedepends=()
provides=()
conflicts=()
replaces=()
backup=()
install=
source=($pkgname-$pkgver.tar.gz)
noextract=()
md5sums=() #generate with 'makepkg -g'

build() {
  cd "$srcdir/$pkgname-$pkgver"
  cp -R ./* $pkgdir/
}