# Pedigree PKGBUILD Script
#
# What:       libarchive
# Is a:       Port
# Maintainer: Matthew Iselin

pkgname=libarchive
pkgver=2.7.0
pkgrel=1
pkgdesc="BSD libarchive - library for several archive formats"
arch=('i686')
url=""
license=('New BSD License')
groups=()
depends=()
makedepends=()
provides=()
conflicts=()
replaces=()
backup=()
install=
source=($pkgname-$pkgver.tar.gz)
noextract=()
md5sums=() #generate with 'makepkg -g'

build() {
  cd "$srcdir/$pkgname-$pkgver"
  cp -R ./* $pkgdir/
}