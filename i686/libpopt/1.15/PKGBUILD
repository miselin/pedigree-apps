# Pedigree PKGBUILD Script
#
# What:       popt
# Is a:       Port
# Maintainer: Matthew Iselin

pkgname=popt
pkgver=1.15
pkgrel=1
pkgdesc="popt library runtime"
arch=('i686')
url=""
license=('X11')
groups=()
depends=('gettext')
makedepends=()
provides=()
conflicts=()
replaces=()
backup=()
install=
source=($pkgname-$pkgver.tar.gz)
noextract=()
md5sums=() #generate with 'makepkg -g'

build() {
  cd "$srcdir/$pkgname-$pkgver"
  cp -R ./* $pkgdir/
}