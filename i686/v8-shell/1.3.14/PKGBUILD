# What:       v8-shell
# Is a:       Port
# Maintainer: Burtescu Eduard

pkgname=v8-shell
pkgver=1.3
pkgrel=14
pkgdesc="A sample shell for v8"
arch=('i686')
url="http://code.google.com/p/v8/"
license=('New BSD License')
groups=()
depends=()
makedepends=()
provides=()
conflicts=()
replaces=()
backup=()
install=
source=($pkgname-$pkgver.tar.gz)
noextract=()
md5sums=() #generate with 'makepkg -g'

build() {
  cd "$srcdir/$pkgname-$pkgver"
  cp -R ./* $pkgdir/
}