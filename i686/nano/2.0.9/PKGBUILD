# Pedigree PKGBUILD Script
#
# What:       nano
# Is a:       Port
# Maintainer: James Molloy

pkgname=nano
pkgver=2.0.9
pkgrel=1
pkgdesc="The nano text editor"
arch=('i686')
url=""
license=('GPL')
groups=()
depends=()
makedepends=()
provides=()
conflicts=()
replaces=()
backup=()
install=
source=($pkgname-$pkgver.tar.gz)
noextract=()
md5sums=() #generate with 'makepkg -g'

build() {
  cd "$srcdir/$pkgname-$pkgver"
  cp -R ./* $pkgdir/
}