# Pedigree PKGBUILD Script
#
# What:       apache
# Is a:       Port
# Maintainer: Matthew Iselin

pkgname=apache
pkgver=2.2.11
pkgrel=1
pkgdesc="The Apache web server"
arch=('i686')
url=""
license=('GPL')
groups=()
depends=()
makedepends=()
provides=()
conflicts=()
replaces=()
backup=()
install=
source=($pkgname-$pkgver.tar.gz)
noextract=()
md5sums=() #generate with 'makepkg -g'

build() {
  cd "$srcdir/$pkgname-$pkgver"
  cp -R ./* $pkgdir/
}