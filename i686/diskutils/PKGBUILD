# Pedigree PKGBUILD Script
#
# What:       Diskutils (du)
# Is a:       Port
# Maintainer: James Molloy

pkgname=diskutils
pkgver=unknown
pkgrel=1
pkgdesc="du command (disk usage), potentially more disk utilities"
arch=('i686')
url=""
license=('GPL')
groups=()
depends=()
makedepends=()
provides=()
conflicts=()
replaces=()
backup=()
install=
source=($pkgname-$pkgver.tar.gz)
noextract=()
md5sums=() #generate with 'makepkg -g'

build() {
  cd "$srcdir/$pkgname-$pkgver"
  cp -R ./* $pkgdir/
}