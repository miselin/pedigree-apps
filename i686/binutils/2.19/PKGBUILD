# Pedigree PKGBUILD Script
#
# What:       binutils
# Is a:       Port
# Maintainer: Matthew Iselin

pkgname=binutils
pkgver=2.19
pkgrel=1
pkgdesc="GNU Binutils"
arch=('i686')
url=""
license=('GPL')
groups=()
depends=()
makedepends=()
provides=()
conflicts=()
replaces=()
backup=()
install=
source=($pkgname-$pkgver.tar.gz)
noextract=()
md5sums=() #generate with 'makepkg -g'

build() {
  cd "$srcdir/$pkgname-$pkgver"
  cp -R ./* $pkgdir/
}
